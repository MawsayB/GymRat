@model GymRat.ViewModels.WorkoutViewModel

<style>
    .button {
        background-color: white;
        color: #3F99DB;
        padding: 10px 20px;
        border-radius: 6px;
        cursor: pointer;
        font-family: 'Century Gothic';
        font-size: 16px;
        font-weight: bold;
        text-decoration: none;
    }

    .block {
        float: left;
    }

    .logo {
        width: 230px;
        height: 230px;
        margin-top: 100px;
    }

    .headline {
        font-size: 48px;
        margin-left: -25px;
    }

    .radio {
        font-size: 30px;
        text-align: left;
    }

    table {
        width: 35%;
        margin-left: auto;
        margin-right: auto;
        border: 4px solid #3F99DB;
    }

        table th {
            font-size: 36px;
            border: 4px solid #3F99DB;
            text-align: center;
        }
</style>

<body>
    <br />

    <div class="col-lg-6">
        <img src="data:image/svg+xml;utf8;base64," class="logo" />
        <p class="headline">Lift</p>
    </div>

    <div class="col-lg-6">
    <h2 class="headline">MUSCLE GROUP</h2>
    <hr />

    <h1>EXERCISE</h1>
    <form asp-controller="Measuring" asp-action="Add" method="post">

        <div class="form-group">
            <input asp-for="Reps" size="10" placeholder="Reps" class="form-control" />
            <span asp-validation-for="Reps"></span>
        </div>

        <h4>(x of 20 remain)</h4>

        <div class="form-group">
            <input asp-for="Weight" size="10" placeholder="Weight" class="form-control" />
            <span asp-validation-for="Weight"></span>
        </div>

        <h4>max: lbs.</h4>

        <div class="form-group">
            <input type="submit" class="button" value="Save" />
        </div>
    </form>
        </div>
</body>